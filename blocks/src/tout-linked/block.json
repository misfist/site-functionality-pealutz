{
    "apiVersion": 2,
    "version": "1.0.0",
    "textdomain": "site-functionality",
    "name": "site-functionality/tout-linked",
    "title": "Linked Tout",
    "icon": "share-alt2",
    "category": "touts",
    "collection": "site-functionality",
    "description": "Display a linked tout on page",
    "keywords": [
        "callout",
        "tout",
        "button"
    ],
    "attributes": {
        "url": {
            "type": "string",
            "default": ""
        },
        "linkTarget": {
			"type": "string"
		},
		"rel": {
			"type": "string"
        },
        "className": {
            "type": "string",
            "default": "tout linked"
        },
        "anchor": {
			"type": "string"
        }
    },
    "supports": {
        "align": true,
        "anchor": true,
        "color": {
            "background": true,
            "text": true,
            "gradients": false,
            "link": false
        },
        "customClassName": true,
        "defaultStylePicker": false,
        "__experimentalLayout": false,
        "__experimentalStyle": false,
        "__experimentalBorder": false
    },
    "example" : {
        "attributes": {
            "className": "tout linked",
            "url": "#"
        },
        "innerBlocks": [
            {
                "name": "core/heading",
                "attributes": {
                    "level": 3,
                    "className": "tout__title",
                    "content": "Donate to the Uru-eu-Wau-Wau Association"
                }
            },
            {
                "name": "core/image",
                "attributes": {
                    "className": "tout__image",
                    "sizeSlug": "medium",
                    "url": "https://images.unsplash.com/photo-1617573211120-1165b16c2900"
                }
            }
        ]
    },
    "style": "file:../../build/style-index.css",
    "editorScript": "site-functionality",
	"editorStyle": "file:../../build/index.css"
}
